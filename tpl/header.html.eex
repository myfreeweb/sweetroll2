<header>
	<h1><a href="/"><%= home(@posts).props["name"] %></a></h1>
	<nav>
		<%= for feed_url <- @feed_urls do %>
			<%= content_tag :a, href: feed_url, rel: "feed", type: "text/html", class: (if feed_url == @cur_url, do: "current", else: "") do %>
				<%= as_one @posts[feed_url].props["name"] %>
			<% end %>
		<% end %>
	</nav>
</header>
